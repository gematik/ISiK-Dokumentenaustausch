{
  "resourceType": "StructureDefinition",
  "id": "SubmitDocumentInput",
  "extension": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-category",
      "valueString": "Foundation.Other"
    }
  ],
  "url": "https://gematik.de/fhir/ISiK/v2/StructureDefinition/SubmitDocumentInput",
  "version": "2.0.0",
  "name": "SubmitDocumentInput",
  "title": "SubmitDocumentInput",
  "status": "draft",
  "experimental": false,
  "date": "2021-11-12",
  "publisher": "gematik GmbH",
  "description": "Profil zur Validierung der Input-Parameter für $submit-document",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Parameters",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Parameters",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Parameters",
        "path": "Parameters",
        "constraint": [
          {
            "key": "sub-in-1",
            "severity": "error",
            "human": "Es muss entweder ein Payload vom Typ Binary oder vom Typ Bundle übermittelt werden!",
            "expression": "parameter.where(name= 'payloadBinary').exists() xor parameter.ahere(name='payloadBundle').exists()",
            "source": "https://gematik.de/fhir/ISiK/v2/StructureDefinition/SubmitDocumentInput"
          }
        ]
      },
      {
        "id": "Parameters.parameter",
        "path": "Parameters.parameter",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "name"
            }
          ],
          "rules": "open"
        },
        "min": 2,
        "mustSupport": true
      },
      {
        "id": "Parameters.parameter:input-mode",
        "path": "Parameters.parameter",
        "sliceName": "input-mode",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Parameters.parameter:input-mode.name",
        "path": "Parameters.parameter.name",
        "patternString": "input-mode"
      },
      {
        "id": "Parameters.parameter:input-mode.value[x]",
        "path": "Parameters.parameter.value[x]",
        "slicing": {
          "discriminator": [
            {
              "type": "type",
              "path": "$this"
            }
          ],
          "ordered": false,
          "rules": "open"
        },
        "type": [
          {
            "code": "code"
          }
        ]
      },
      {
        "id": "Parameters.parameter:input-mode.valueCode",
        "path": "Parameters.parameter.valueCode",
        "min": 0,
        "max": "1",
        "binding": {
          "strength": "required",
          "valueSet": "https://gematik.de/fhir/ISiK/v2/ValueSet/SubmitDocumentModes"
        }
      },
      {
        "id": "Parameters.parameter:input-metadata",
        "path": "Parameters.parameter",
        "sliceName": "input-metadata",
        "short": "Dokumentenmetadaten in Form einer DocumentReference-Ressource",
        "comment": "...",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Parameters.parameter:input-metadata.name",
        "path": "Parameters.parameter.name",
        "patternString": "input-metadata"
      },
      {
        "id": "Parameters.parameter:input-metadata.resource",
        "path": "Parameters.parameter.resource",
        "type": [
          {
            "code": "DocumentReference",
            "profile": [
              "https://gematik.de/fhir/ISiK/v2/StructureDefinition/ISiKDokumentenMetadaten"
            ]
          }
        ]
      },
      {
        "id": "Parameters.parameter:payloadBinary",
        "path": "Parameters.parameter",
        "sliceName": "payloadBinary",
        "short": "unstrukturiertes oder nicht FHIR-basiertes Dokument",
        "definition": "Wenn ein unstrukturiertes oder nicht FHIR-basiertes Dokument (z.B. PDF, Word, CDA) übermittelt werden soll, dann muss dieses im Parameter `payloadBundle` übermittelt werden",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Parameters.parameter:payloadBinary.name",
        "path": "Parameters.parameter.name",
        "patternString": "payloadBinary"
      },
      {
        "id": "Parameters.parameter:payloadBinary.resource",
        "path": "Parameters.parameter.resource",
        "type": [
          {
            "code": "Binary"
          }
        ]
      },
      {
        "id": "Parameters.parameter:payloadBundle",
        "path": "Parameters.parameter",
        "sliceName": "payloadBundle",
        "short": "strukturiertes, FHIR-basiertes Dokument",
        "definition": "Wenn ein strukturiertes, FHIR-basiertes Dokument (Bundle vom Typ `document`) übermittelt werden soll, dann muss dieses im Parameter `payloadBundle` übermittelt werden",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Parameters.parameter:payloadBundle.name",
        "path": "Parameters.parameter.name",
        "patternString": "payloadBundle"
      },
      {
        "id": "Parameters.parameter:payloadBundle.resource",
        "path": "Parameters.parameter.resource",
        "type": [
          {
            "code": "Bundle"
          }
        ]
      },
      {
        "id": "Parameters.parameter:payloadBundle.resource.type",
        "path": "Parameters.parameter.resource.type",
        "patternCode": "document"
      }
    ]
  }
}
